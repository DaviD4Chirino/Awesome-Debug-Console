; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Awesome Debug Console"
config/features=PackedStringArray("4.2")
config/icon="res://icon.svg"

[autoload]

SaveSystem="*res://addons/save_system/save_system.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1024
window/size/window_height_override=576
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Awesome Godot Plugin Template"

[editor_plugins]

enabled=PackedStringArray("res://addons/awesome_debug_console/plugin.cfg", "res://addons/nv.file_system/plugin.cfg", "res://addons/save_system/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
